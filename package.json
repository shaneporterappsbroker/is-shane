{
  "name": "@shaneporterappsbroker/is-shane",
  "version": "1.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "check": "npm run check:ts && npm run check:lint",
    "check:ts": "tsc --noEmit",
    "check:lint": "eslint \"**/*.ts\" \"**/*.js\" --max-warnings=0",
    "check:lint:fix": "npm run check:lint -- --fix",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaneporterappsbroker/is-shane.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/shaneporterappsbroker"
  },
  "author": "Shane Porter",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "shane",
    "string"
  ],
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "docz": "^1.0.3",
    "docz-theme-default": "^1.0.3",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.6.1",
    "prettier": "^2.1.1",
    "semantic-release": "^17.1.1",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3"
  },
  "release": {
    "plugins": [
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "branches": [
      "main"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {}
}
